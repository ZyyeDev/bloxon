[sub_resource type="GDScript" id="GDScript_ouet7"]
script/source = "extends ScrollContainer

@onready var vbar := get_v_scroll_bar()
@onready var vbox := $VBoxContainer

var always_autoscroll: bool = true

func _ready() -> void:
	vbox.child_order_changed.connect(_on_messages_updated)
	vbox.minimum_size_changed.connect(_on_messages_updated)

func _on_messages_updated() -> void:
	if always_autoscroll:
		scroll_vertical = vbar.max_value
	else:
		if scroll_vertical >= vbar.max_value - vbar.page - 10:
			scroll_vertical = vbar.max_value

func add_message(text: String) -> void:
	var label := RichTextLabel.new()
	label.bbcode_enabled = true
	label.text = text
	vbox.add_child(label)
"

